<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atlisheng.eduuser.mapper.EduUserMapper">

    <select id="queryRegisterCountDaily" resultType="java.lang.Integer">
        SELECT
           COUNT(1)
        FROM
            edu_user
        WHERE
            DATE(gmt_create) = #{day}
    </select>
</mapper>
